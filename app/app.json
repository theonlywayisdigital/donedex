{
  "expo": {
    "name": "Donedex",
    "slug": "donedex",
    "scheme": "donedex",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/donedex-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/donedex-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A3D4F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.donedex.app",
      "associatedDomains": ["applinks:donedex-app.netlify.app"],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/donedex-icon.png",
        "backgroundColor": "#0A3D4F"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.donedex.app",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/donedex-icon.png",
      "output": "single",
      "name": "Donedex"
    },
    "plugins": [
      "@react-native-community/datetimepicker",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "Donedex needs access to your photos to attach images to inspections.",
          "cameraPermission": "Donedex needs access to your camera to take photos during inspections."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Donedex needs access to your camera to take photos during inspections."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/donedex-icon.png",
          "color": "#0F4C5C"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6047da36-61fb-4c60-992f-097a9185156b"
      }
    },
    "owner": "theonlywayisdigital"
  }
}
